{
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    { "source": "/guide", "destination": "/page1-leadmagnet.html" },
    { "source": "/confirmation", "destination": "/page2-confirmation.html" }
  ],
  "headers": [
    {
      "source": "/images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ]
}
